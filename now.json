{
  "version": 2,
  "public": true,
  "name": "portfolio",
  "builds": [
    {
      "use": "@now/next"
      "src": "packages/next-main/package.json",
    },
    {
      "use": "@now/next"
      "src": "packages/next-blog/package.json",
    },
    {
      "src": "packages/api/apollo/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "^/blog(.*)",
      "dest": "packages/next-blog$1"
    },
    {
      "src": "/graphql(.*)",
      "dest": "packages/api/apollo/index.js"
    },
    {
      "src": "^(.*)$",
      "dest": "packages/next-main$1"
    }
  ],
  "regions": ["all"],
  "alias": "fillipvt.com"
}