{
  "name": "@fillipvt/components",
  "private": false,
  "version": "0.0.1",
  "description": "Library of shared components",
  "author": "fillipvt",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn build:es && yarn build:umd && yarn build:copy-files",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ./babel.config.js ./src --out-dir ./build --ignore \"**/*.test.js\"",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ./babel.config.js ./src --out-dir ./build/esm --ignore \"**/*.test.js\"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./build/es --ignore \"**/*.test.js\"",
    "build:umd": "cross-env BABEL_ENV=production-umd rollup -c scripts/rollup.config.js",
    "build:copy-files": "node ./scripts/copy-files.js",
    "prebuild": "rimraf build",
    "release": "yarn build && npm publish build",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "@storybook/addon-actions": "5.0.0-rc.3",
    "@storybook/addon-backgrounds": "4.1.12",
    "@storybook/addon-centered": "5.0.0-rc.3",
    "@storybook/addon-console": "1.1.0",
    "@storybook/addon-info": "5.0.0-rc.3",
    "@storybook/addon-jest": "5.0.0-rc.3",
    "@storybook/addon-knobs": "5.0.0-rc.3",
    "@storybook/addon-links": "5.0.0-rc.3",
    "@storybook/addon-notes": "5.0.0-rc.3",
    "@storybook/addon-options": "5.0.0-rc.3",
    "@storybook/addon-viewport": "4.1.12",
    "@storybook/addons": "5.0.0-rc.3",
    "@storybook/components": "4.1.12",
    "@storybook/react": "5.0.0-rc.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "10.0.7",
    "babel-plugin-module-resolver": "^3.1.3",
    "babel-plugin-transform-dev-warning": "^0.1.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "cross-env": "^5.2.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "rimraf": "^2.6.3",
    "rollup": "1.2.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-size-snapshot": "^0.8.0",
    "rollup-plugin-terser": "^4.0.4"
  },
  "main": "./src/index",
  "module": "./src/esm/index",
  "sideEffects": false,
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "10.0.7",
    "@emotion/styled": "10.0.7",
    "@fortawesome/fontawesome-svg-core": "1.2.15",
    "@fortawesome/free-brands-svg-icons": "5.7.2",
    "@fortawesome/free-regular-svg-icons": "5.7.2",
    "@fortawesome/free-solid-svg-icons": "5.7.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-spring": "7.2.10"
  },
  "peerDependencies": {
    "@emotion/core": ">=10.0.7",
    "@emotion/styled": ">=10.0.7",
    "@fortawesome/fontawesome-svg-core": ">=1.2.15",
    "@fortawesome/free-brands-svg-icons": ">=5.7.2",
    "@fortawesome/free-regular-svg-icons": ">=5.7.2",
    "@fortawesome/free-solid-svg-icons": ">=5.7.2",
    "@fortawesome/react-fontawesome": ">=0.1.4",
    "react": ">=16.8.2",
    "react-dom": ">=16.8.2",
    "react-spring": ">=8.0.7"
  }
}
